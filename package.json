    "core-js": "^3.6.5",
    "element-ui": "^2.14.1",
    "express": "^4.17.1",
    "express-locale": "^2.0.0",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.15",
    "nodemailer": "^6.4.16",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^4.0.0-beta.1",
    "webpack": "^5.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
